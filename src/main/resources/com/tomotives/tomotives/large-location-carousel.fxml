<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane fx:id="carouselRoot" prefHeight="250.0" prefWidth="600.0" stylesheets="@carousel.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tomotives.tomotives.controllers.LargeLocationCarouselController">
    <children>
        <VBox alignment="TOP_CENTER" spacing="15.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox alignment="CENTER" spacing="35.0">
                    <children>
                        <Button fx:id="prevButton" onAction="#goToPrevious" styleClass="carousel-button" text="&lt;">
                            <font>
                                <Font name="System Bold" size="18.0" />
                            </font>
                        </Button>
                        <fx:include fx:id="largeLocationDisplay" source="large-location-display.fxml" />
                        <Button fx:id="nextButton" onAction="#goToNext" styleClass="carousel-button" text="&gt;">
                            <font>
                                <Font name="System Bold" size="18.0" />
                            </font>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
